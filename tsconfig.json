{
  "compilerOptions": {
    "module": "Node16",
    "target": "ES2021",
    "lib": ["ES2021"],
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./"
  },
  "include": ["./**/*.ts"]
}